#! /bin/bash
#PBS -N qconda
#PBS -P a56
#PBS -l storage=gdata/a56
#PBS -S /bin/bash
#PBS -j oe

# Logging
echo "Execution directory: $cwd"
echo "Conda environment: $env"
echo "Command: $cmd"

# Strict mode (exit on fail)
set -ex

# Activate conda environment
eval "$(conda shell.bash hook)"
conda activate $env
# Switch work directory
cd $cwd
# Run command
eval $cmd 
